<%- include('layouts/header')  %>
		<h1>Táblázatok</h1>
		<div class="tablazatok">
			<% if (oktato) { %>
				<h2>Oktató <%= oktato  %> </h2>
				<table>
					<% holTanit.forEach(element => { %>
						<tr>
							<% if (element.startsWith('0')) { %>
								<td><a href="/tablazat/?cim=Oktató_<%= oktato %>&hol=<%= element.substring(1) %>"><%= element.substring(1) %></a></td>
							<% } else { %>
								<td><a href="/tablazat/?cim=Oktató_<%= oktato %>&hol=<%= element %>"><%= element %></a></td>
							<% } %> 
						</tr>
					<% })%>
				</table>
			<% } else { %>
				<h2><%= cim %> </h2>
				<table>
					<% holTanit.forEach(element => { %>
						<tr>
							<td><a href="/tablazat/?cim=<%= cim %>&hol=<%= element %>"><%= element %></a></td> 
						</tr>
					<% })%>
				</table>
			<% } %>
		</div>
	</body>
</html>